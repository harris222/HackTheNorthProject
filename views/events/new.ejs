  <div class="container-fluid" id="fluid">
      <div id="registrationForm">
        <h1 id = "titleText">Create an Event</h1>
        <div class="row">
            <div class="col">
                <form action="/events" method="POST">
                    <div class="form-group">
                       <div class="labels"><label for="name">Event Name:</label></div>
                       <input type="text" class="form-control" name="name" aria-describedby="emailHelp" placeholder="Enter Event Name">
                    </div>

                    <div class="form-group">
                       <div class="labels"><label for="image">Image:</label></div>
                       <input type="text" class="form-control" name="image" aria-describedby="emailHelp" placeholder="Enter your URL">
                    </div>

                    <div class="form-group">
                       <div class="labels"><label for="location">Location:</label></div>
                       <input type="location" class="form-control" name="location" aria-describedby="emailHelp" placeholder="Enter Event location">
                    </div>

                    <div class="form-group">
                      <div class="labels"><label for="discription">Description: </label></div>
                      <div id="description"><textarea class="form-control" id="description" name="description" placeholder="Leave something about the event you are holding..."></textarea></div>
                    </div>

                    <button class="btn btn-primary btn-block">Create Event</button>
                </form>
            </div>
        </div>
      </div>
  </div>

      
      
      
